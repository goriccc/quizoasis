export function calculateTeamPlayerResult(answers: string[], questions: TeamPlayerQuestion[]): TeamPlayerResult {
  const scores = {
    Type1: 0,
    Type2: 0,
    Type3: 0,
    Type4: 0,
    Type5: 0,
    Type6: 0
  };

  // ê° ì§ˆë¬¸ë³„ ì ìˆ˜ ê³„ì‚°
  answers.forEach((answer, index) => {
    const question = questions[index];
    
    switch (question.id) {
      case 1:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') scores.Type2 += 3;
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 2:
        if (answer === 'A') { scores.Type1 += 3; scores.Type6 += 2; }
        else if (answer === 'B') { scores.Type2 += 3; scores.Type5 += 2; }
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') { scores.Type6 += 8; scores.Type4 += 2; }
        break;
      case 3:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') scores.Type2 += 3;
        else if (answer === 'C') { scores.Type3 += 3; scores.Type6 += 2; }
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 4:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') { scores.Type5 += 8; scores.Type2 += 2; }
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 5:
        if (answer === 'A') { scores.Type1 += 3; scores.Type5 += 2; }
        else if (answer === 'B') scores.Type2 += 3;
        else if (answer === 'C') { scores.Type3 += 3; scores.Type6 += 2; }
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 6:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') { scores.Type2 += 3; scores.Type5 += 2; }
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 7:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') { scores.Type5 += 8; scores.Type2 += 2; }
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 8:
        if (answer === 'A') { scores.Type6 += 8; scores.Type1 += 2; }
        else if (answer === 'B') scores.Type2 += 3;
        else if (answer === 'C') { scores.Type3 += 3; scores.Type5 += 2; }
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 9:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') scores.Type2 += 3;
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 10:
        if (answer === 'A') scores.Type1 += 3;
        else if (answer === 'B') { scores.Type2 += 3; scores.Type5 += 2; }
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') { scores.Type6 += 8; scores.Type4 += 2; }
        break;
      case 11:
        if (answer === 'A') { scores.Type6 += 8; scores.Type1 += 2; }
        else if (answer === 'B') scores.Type2 += 3;
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
      case 12:
        if (answer === 'A') { scores.Type1 += 3; scores.Type6 += 2; }
        else if (answer === 'B') { scores.Type5 += 8; scores.Type2 += 2; }
        else if (answer === 'C') scores.Type3 += 3;
        else if (answer === 'D') scores.Type4 += 3;
        break;
    }
  });

  // ìµœê³  ì ìˆ˜ ì°¾ê¸°
  const maxScore = Math.max(...Object.values(scores));
  const maxScoreTypes = Object.keys(scores).filter(type => scores[type as keyof typeof scores] === maxScore);
  
  let resultType = maxScoreTypes[0];
  
  // ë™ì ì¼ ê²½ìš° Q10-Q12ì˜ ì„ íƒì„ ìš°ì„  ë°˜ì˜ (ì‚¬ìš©ì ì‹œë‚˜ë¦¬ì˜¤ì— ë”°ë¼)
  if (maxScoreTypes.length > 1) {
    // Q10, Q11, Q12ì˜ ë‹µë³€ì„ í™•ì¸í•˜ì—¬ ìš°ì„ ìˆœìœ„ ê²°ì •
    const priorityAnswers = answers.slice(9, 12); // Q10, Q11, Q12
    
    for (let i = 0; i < priorityAnswers.length; i++) {
      const answer = priorityAnswers[i];
      const questionNum = 10 + i;
      
      // ê° ì§ˆë¬¸ì˜ ë‹µë³€ì— ë”°ë¥¸ ìš°ì„ ìˆœìœ„ íƒ€ì… ê²°ì •
      if (questionNum === 10) {
        if (answer === 'A' && maxScoreTypes.includes('Type1')) {
          resultType = 'Type1';
          break;
        } else if (answer === 'B' && maxScoreTypes.includes('Type2')) {
          resultType = 'Type2';
          break;
        } else if (answer === 'C' && maxScoreTypes.includes('Type3')) {
          resultType = 'Type3';
          break;
        } else if (answer === 'D' && maxScoreTypes.includes('Type4')) {
          resultType = 'Type4';
          break;
        }
      } else if (questionNum === 11) {
        if (answer === 'A' && maxScoreTypes.includes('Type6')) {
          resultType = 'Type6';
          break;
        } else if (answer === 'B' && maxScoreTypes.includes('Type2')) {
          resultType = 'Type2';
          break;
        } else if (answer === 'C' && maxScoreTypes.includes('Type3')) {
          resultType = 'Type3';
          break;
        } else if (answer === 'D' && maxScoreTypes.includes('Type4')) {
          resultType = 'Type4';
          break;
        }
      } else if (questionNum === 12) {
        if (answer === 'A' && maxScoreTypes.includes('Type1')) {
          resultType = 'Type1';
          break;
        } else if (answer === 'B' && maxScoreTypes.includes('Type5')) {
          resultType = 'Type5';
          break;
        } else if (answer === 'C' && maxScoreTypes.includes('Type3')) {
          resultType = 'Type3';
          break;
        } else if (answer === 'D' && maxScoreTypes.includes('Type4')) {
          resultType = 'Type4';
          break;
        }
      }
    }
  }

  // ê²°ê³¼ ë°˜í™˜
  const result = teamPlayerResults.find(r => r.type === resultType);
  return result || teamPlayerResults[0];
}
